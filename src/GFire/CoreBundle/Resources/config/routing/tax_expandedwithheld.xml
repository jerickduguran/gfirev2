<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="tax_expandedwithheld" path="/">
        <default key="_controller">GFireCoreBundle:Tax\ExpandedWithheld:index</default>
    </route>

    <route id="tax_expandedwithheld_show" path="/{id}/show">
        <default key="_controller">GFireCoreBundle:Tax\ExpandedWithheld:show</default>
    </route>

    <route id="tax_expandedwithheld_new" path="/new">
        <default key="_controller">GFireCoreBundle:Tax\ExpandedWithheld:new</default>
    </route>

    <route id="tax_expandedwithheld_create" path="/create">
        <default key="_controller">GFireCoreBundle:Tax\ExpandedWithheld:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="tax_expandedwithheld_edit" path="/{id}/edit">
        <default key="_controller">GFireCoreBundle:Tax\ExpandedWithheld:edit</default>
    </route>

    <route id="tax_expandedwithheld_update" path="/{id}/update">
        <default key="_controller">GFireCoreBundle:Tax\ExpandedWithheld:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="tax_expandedwithheld_delete" path="/{id}/delete">
        <default key="_controller">GFireCoreBundle:Tax\ExpandedWithheld:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>
