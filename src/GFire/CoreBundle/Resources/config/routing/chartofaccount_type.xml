<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="chartofaccount_type" path="/">
        <default key="_controller">GFireCoreBundle:ChartOfAccount\Type:index</default>
    </route>

    <route id="chartofaccount_type_new" path="/new">
        <default key="_controller">GFireCoreBundle:ChartOfAccount\Type:new</default>
    </route>

    <route id="chartofaccount_type_create" path="/create">
        <default key="_controller">GFireCoreBundle:ChartOfAccount\Type:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="chartofaccount_type_edit" path="/{id}/edit">
        <default key="_controller">GFireCoreBundle:ChartOfAccount\Type:edit</default>
    </route>

    <route id="chartofaccount_type_update" path="/{id}/update">
        <default key="_controller">GFireCoreBundle:ChartOfAccount\Type:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="chartofaccount_type_delete" path="/{id}/delete">
        <default key="_controller">GFireCoreBundle:ChartOfAccount\Type:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>
