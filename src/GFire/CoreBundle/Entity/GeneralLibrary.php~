    /**
     * @var \Doctrine\Common\Collections\Collection
     */
    private $finalWithheldTaxes;

    /**
     * Constructor
     */
    public function __construct()
    {
        $this->finalWithheldTaxes = new \Doctrine\Common\Collections\ArrayCollection();
    }

    /**
     * Add finalWithheldTaxes
     *
     * @param \GFire\CoreBundle\Entity\Tax\FinalWithheld $finalWithheldTaxes
     * @return GeneralLibrary
     */
    public function addFinalWithheldTax(\GFire\CoreBundle\Entity\Tax\FinalWithheld $finalWithheldTaxes)
    {
        $this->finalWithheldTaxes[] = $finalWithheldTaxes;

        return $this;
    }

    /**
     * Remove finalWithheldTaxes
     *
     * @param \GFire\CoreBundle\Entity\Tax\FinalWithheld $finalWithheldTaxes
     */
    public function removeFinalWithheldTax(\GFire\CoreBundle\Entity\Tax\FinalWithheld $finalWithheldTaxes)
    {
        $this->finalWithheldTaxes->removeElement($finalWithheldTaxes);
    }

    /**
     * Get finalWithheldTaxes
     *
     * @return \Doctrine\Common\Collections\Collection 
     */
    public function getFinalWithheldTaxes()
    {
        return $this->finalWithheldTaxes;
    }
