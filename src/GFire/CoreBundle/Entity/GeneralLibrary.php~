     * @param \GFire\CoreBundle\Entity\Tax\ExpandedWithheld $expandedWithheldTaxes
     */
    public function removeExpandedWithheldTax(\GFire\CoreBundle\Entity\Tax\ExpandedWithheld $expandedWithheldTaxes)
    {
        $this->expandedWithheldTaxes->removeElement($expandedWithheldTaxes);
    }

    /**
     * Get expandedWithheldTaxes
     *
     * @return \Doctrine\Common\Collections\Collection 
     */
    public function getExpandedWithheldTaxes()
    {
        return $this->expandedWithheldTaxes;
    }


    public function prePersist()
    {
        $this->createdAt = new \DateTime();
    }
    public function postPersist()
    {
        $this->updatedAt = new \DateTime();
    }

    /**
