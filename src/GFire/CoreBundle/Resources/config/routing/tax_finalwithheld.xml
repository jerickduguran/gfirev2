<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="tax_finalwithheld" path="/">
        <default key="_controller">GFireCoreBundle:Tax\FinalWithheld:index</default>
    </route>

    <route id="tax_finalwithheld_show" path="/{id}/show">
        <default key="_controller">GFireCoreBundle:Tax\FinalWithheld:show</default>
    </route>

    <route id="tax_finalwithheld_new" path="/new">
        <default key="_controller">GFireCoreBundle:Tax\FinalWithheld:new</default>
    </route>

    <route id="tax_finalwithheld_create" path="/create">
        <default key="_controller">GFireCoreBundle:Tax\FinalWithheld:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="tax_finalwithheld_edit" path="/{id}/edit">
        <default key="_controller">GFireCoreBundle:Tax\FinalWithheld:edit</default>
    </route>

    <route id="tax_finalwithheld_update" path="/{id}/update">
        <default key="_controller">GFireCoreBundle:Tax\FinalWithheld:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="tax_finalwithheld_delete" path="/{id}/delete">
        <default key="_controller">GFireCoreBundle:Tax\FinalWithheld:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>
